{
  "version": 2,
  "title": "Next.js landing pages",
  "description": "SEO friendly page builder in React.js. Heroes, sign-up forms and calls to action.",
  "previewMedia": {
    "type": "image",
    "src": ".sanity-template/assets/frontend.jpg",
    "alt": "A preview image of the Next.js landing pages website."
  },
  "technologies": [
    {
      "id": "nextjs",
      "name": "Next.js",
      "url": "https://nextjs.org/"
    },
    {
      "id": "vercel",
      "name": "Vercel",
      "url": "https://vercel.com/"
    }
  ],
  "deployment": {
    "provider": "vercel",
    "studio": {"basePath": "/studio"},
    "corsOrigins": [
      {
        "origin": "http://localhost:3000",
        "allowCredentials": true
      }
    ],
    "envVars": {
      "projectId": ["NEXT_PUBLIC_SANITY_PROJECT_ID"],
      "dataset": ["NEXT_PUBLIC_SANITY_DATASET"]
    }
  }
}
